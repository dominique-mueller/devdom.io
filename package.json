{
  "name": "devdom.io",
  "description": "My personal website.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dominique-mueller/devdom.io"
  },
  "type": "module",
  "scripts": {
    "build": "node run-executable bin/hugo/hugo --config=hugo.config.json",
    "postinstall": "hugo-installer --version binDependencies.hugo",
    "start": "node run-executable bin/hugo/hugo --config=hugo.config.json --watch"
  },
  "binDependencies": {
    "hugo": "0.82.0"
  },
  "devDependencies": {
    "hugo-installer": "3.0.x"
  }
}
